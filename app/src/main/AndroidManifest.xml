<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.INTERNET" />


    <permission
        android:name="${applicationId}.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodODoor"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".activites.OrderDelivering"
            android:exported="false" />
        <activity
            android:name=".activites.OrderConfirmedActivity"
            android:exported="false" />
        <activity
            android:name=".activites.ServiceProviderFullListActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".activites.WebActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.CartActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.SpleshActivity"
            android:exported="true"
            android:noHistory="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.OrderDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.NotificationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.AddAddressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.ComplainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.WishlistActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.PaymentSummaryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.DeliveryOptionsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.DeliveryAddressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.ProductDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.SearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activites.MainActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="cat.ereza.customactivityoncrash.activity.DefaultErrorActivity"
            android:process=":error_activity"
            android:theme="@style/Theme.FoodODoor" />

    </application>

</manifest>